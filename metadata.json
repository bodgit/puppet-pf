{
  "name": "bodgit-pf",
  "version": "1.0.0",
  "author": "bodgit",
  "summary": "Puppet Module for managing PF",
  "license": "Apache-2.0",
  "source": "https://github.com/bodgit/puppet-pf",
  "project_page": "https://github.com/bodgit/puppet-pf",
  "issues_url": "https://github.com/bodgit/puppet-pf/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "6.2",
        "6.3"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.5.0 <6.0.0"
    }
  ],
  "tags": [
    "pf",
    "authpf"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.13.0 <5.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=2.1.0 <5.0.0"
    }
  ]
}
